{
    "Sample": {
      "Title": "Node.js console application calling Microsoft Graph with application permissions to manage Azure AD B2C user accounts",
      "Level": 300,
      "Client": "Vanilla JavaScript SPA",
      "Service": "Microsoft Graph",
      "RepositoryUrl": "https://github.com/Azure-Samples/ms-identity-b2c-javascript-nodejs-management",
      "Endpoint": "AAD v2.0",
      "Provider": "B2C"
    },

    "AADApps": [
      {
        "Id": "spa",
        "Name": "ms-identity-javascript-c1s1",
        "Kind" :  "Daemon",
        "Audience": "AzureADMyOrg",
        "HomePage": "http://localhost:5000",
        "RequiredResourcesAccess": [
            {
              "Resource": "Microsoft Graph",
              "ApplicationPermissions": [ "User.Read.All" ]
            }
          ]
      }
    ],
  
    "CodeConfiguration": [
      {
        "App": "spa",
        "SettingKind": "Replace",
        "SettingFile": "\\..\\App\\authConfig.js",
        "Mappings": [
          {
            "key": "Enter_the_Application_Id_Here",
            "value": ".AppId"
          },
          {
            "key": "Enter_the_Cloud_Instance_Id_Here/Enter_the_Tenant_Info_Here",
            "value": "\"https://login.microsoftonline.com/\"+$tenantName"
          }
        ]
      }
    ]
  }